@page "/"
@page "/{Code}"
@page "/{Code}/{Param1}"
@page "/{Code}/{Param1}/{Param2}"
@page "/{Code}/{Param1}/{Param2}/{Param3}"
@page "/{Code}/{Param1}/{Param2}/{Param3}/{Param4}"
@page "/{Code}/{Param1}/{Param2}/{Param3}/{Param4}/{Param5}"
@page "/{Code}/{Param1}/{Param2}/{Param3}/{Param4}/{Param5}/{Param6}"

@if (!string.IsNullOrWhiteSpace(Code))
{
    <DynamicSection Code="@Code"
                    Param1="@Param1"
                    Param2="@Param2"
                    Param3="@Param3"
                    Param4="@Param4"
                    Param5="@Param5"
                    Param6="@Param6" />
}
else
{
    <DynamicSection Code="INICIO" />
}

@code {
    [Parameter]
    [SupplyParameterFromQuery]
    public string? Code { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Param1 { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Param2 { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Param3 { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Param4 { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Param5 { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Param6 { get; set; }
}